{
    "parameters": {
        "PageParameter": {
            "name": "page",
            "in": "query",
            "description": "Nomor halaman untuk pagination",
            "required": false,
            "schema": {
                "type": "integer",
                "minimum": 1,
                "default": 1
            }
        },
        "PerPageParameter": {
            "name": "per_page",
            "in": "query",
            "description": "Jumlah item per halaman",
            "required": false,
            "schema": {
                "type": "integer",
                "minimum": 1,
                "maximum": 100,
                "default": 15
            }
        },
        "SearchParameter": {
            "name": "search",
            "in": "query",
            "description": "Kata kunci pencarian",
            "required": false,
            "schema": {
                "type": "string",
                "minLength": 3
            }
        }
    }
}
